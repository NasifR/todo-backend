{
    "rewrites": [
      { "source": "/api/:path*", "destination": "/api/index.js" }
    ],
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }